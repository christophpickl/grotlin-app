        <!--<init-param>-->
            <!--<param-name>javax.ws.rs.Application</param-name>-->
            <!--<param-value>at.cpickl.grotlin.multi.SwirlApplication</param-value>-->
        <!--</init-param>-->
    </servlet>
    <servlet-mapping>
        <servlet-name>resteasyServlet</servlet-name>
        <url-pattern>/*</url-pattern>
